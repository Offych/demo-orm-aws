generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model APIKEY {
  id                String   @id @default(uuid())
  apiKey            String
  apiSecret         String
  environment       String
  project           String
  team              String
  provider          String
  modelType         String
  reminderToUpgrade Boolean
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
}
